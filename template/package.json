{
  "name": "@adobe/franklin-service",
  "version": "1.4.9",
  "private": true,
  "description": "Example Franklin Service",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "c8 mocha  -i -g 'Post-Deploy'",
    "test-postdeploy": "mocha -g 'Post-Deploy'",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --branches $CI_BRANCH",
    "commit": "git-cz",
    "build": "hedy -v",
    "deploy": "hedy -v --deploy --test",
    "deploy-routes": "hedy --no-build --no-hints -l major",
    "deploy-ci": "hedy -v --deploy --test --pkgVersion=ci$CI_BUILD_NUM -l ci --cleanup-ci 24h",
    "prepare": "husky"
  },
  "wsk": {
    "nodeVersion": 18,
    "target": "aws",
    "name": "franklin-services/service@${version}",
    "testUrl": "/_status_check/healthcheck.json",
    "memory": 256,
    "fastlyServiceId!important": ""
  },
  "mocha": {
    "spec": "test/**/*.test.js",
    "require": "test/setup-env.js",
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/franklin-service"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/franklin-service/issues"
  },
  "homepage": "https://github.com/adobe/franklin-service#readme",
  "dependencies": {
    "@adobe/fetch": "4.2.2",
    "@adobe/helix-shared-wrap": "2.0.2",
    "@adobe/helix-status": "10.1.5"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.2",
    "@adobe/helix-deploy": "13.0.3",
    "@adobe/helix-universal": "5.2.2",
    "@adobe/semantic-release-coralogix": "1.1.37",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "c8": "10.1.3",
    "dotenv": "16.5.0",
    "eslint": "8.57.1",
    "husky": "9.1.7",
    "junit-report-builder": "5.1.1",
    "lint-staged": "16.1.0",
    "mocha": "11.6.0",
    "mocha-multi-reporters": "1.5.1",
    "semantic-release": "24.2.5"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
